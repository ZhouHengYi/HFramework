<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://zhy.seo.sh.cn/DataOperation">
  <dataCommand name="Access_Login" database="NCService_Access" commandType="Text">
    <commandText>
      <![CDATA[        
			SELECT * FROM [SystemUser] WHERE UserName = @UserName AND Password = @Password
			]]>
    </commandText>
    <parameters>
      <param name="@UserName" dbType="AnsiStringFixedLength" />
      <param name="@Password" dbType="AnsiStringFixedLength" />
    </parameters>
  </dataCommand>
</dataOperations>
