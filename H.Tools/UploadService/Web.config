<?xml version="1.0"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细信息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
    <httpRuntime executionTimeout="6000" maxRequestLength="102400" useFullyQualifiedRedirectUrl="false"/>
  </system.web>
  <appSettings>
    <!-- 图片保存路径 -->
    <add key="ImageUploadPath" value="images/"/>
  </appSettings>
</configuration>