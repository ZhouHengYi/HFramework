<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.newegg.com/DataOperation">
  <dataCommand name="CreateLogo" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[        
			INSERT INTO [Center].[dbo].[ErrorLog]
           ([LogID]
           ,[GlobalName]
           ,[LocalName]
           ,[Content]
           ,[LogUserName]
           ,[Category]
           ,[LogServerIP]
           ,[LogServerName]
           ,[ReferenceKey])
     VALUES
           (@LogID
           ,@GlobalName
           ,@LocalName
           ,@Content
           ,@LogUserName
           ,@Category
           ,@LogServerIP
           ,@LogServerName
           ,@ReferenceKey)
			]]>
    </commandText>
    <parameters>
      <param name="@LogID" dbType="AnsiStringFixedLength" />
      <param name="@GlobalName" dbType="AnsiStringFixedLength" />
      <param name="@LocalName" dbType="AnsiStringFixedLength" />
      <param name="@Content" dbType="AnsiStringFixedLength" />
      <param name="@LogUserName" dbType="AnsiStringFixedLength" />
      <param name="@Category" dbType="AnsiStringFixedLength" />
      <param name="@LogServerIP" dbType="AnsiStringFixedLength" />
      <param name="@LogServerName" dbType="AnsiStringFixedLength" />
      <param name="@ReferenceKey" dbType="AnsiStringFixedLength" />
    </parameters>
  </dataCommand>
</dataOperations>
